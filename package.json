{
	"name": "vscode-amazing-icons",
	"displayName": "VSCode Amazing Icons",
	"description": "Project based on vscode-great-icons",
	"version": "1.0.0",
	"icon": "images/logo.png",
	"license": "MIT",
	"scripts": {
		"publish": "npm run patch",
    "patch": "vsce publish patch && npm run relase",
		"release": "git tag -a v$(node -p \"require('./package.json').version\") -m \"Release version $(node -p \"require('./package.json').version\")\" && git push origin v$(node -p \"require('./package.json').version\")"
	},
	"devDependencies": {
		"@vscode/vsce": "^3.1.1"
	},
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Other",
		"Themes"
	],
	"contributes": {
		"iconThemes": [
			{
				"id": "vscode-amazing-icons",
				"label": "VSCode Amazing Icons",
				"path": "./icons.json"
			}
		]
	},
	"galleryBanner": {
		"color": "#e0e0e0",
		"theme": "light"
	},
	"keywords": [
		"icons",
		"theme",
		"icon-theme",
		"customization"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/algab/vscode-amazing-icons"
	}
}
